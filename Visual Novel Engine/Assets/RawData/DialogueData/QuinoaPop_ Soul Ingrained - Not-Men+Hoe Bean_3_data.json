        {
            "id": 49,
            "route": "",
            "speakerName": "Bex",
            "sprite": "Angry",
            "text": "Fuck you.",
            "choices": ""
        },
        {
            "id": 50,
            "route": "",
            "speakerName": "",
            "sprite": "",
            "text": "You watch Hoe Bean sprint inside and dance in the doorway - Well, I guess crouch and stand repeatedly by the doorway?",
