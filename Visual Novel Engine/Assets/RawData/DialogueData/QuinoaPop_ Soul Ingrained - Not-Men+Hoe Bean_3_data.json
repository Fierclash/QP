        {
            "id": 32,
            "route": "",
            "speakerName": "You",
            "sprite": "",
            "text": "I got it. Follow me, tomodachi.",
            "choices": ""
        },
        {
            "id": 33,
            "route": "",
            "speakerName": "",
            "sprite": "",
            "text": "You lead your group toward safety, away from the darkness, to take temporary solace in the truck.",
